{
  "name": "@adenta/mdx-bundler",
  "version": "0.0.1",
  "type": "module",
  "types": "./lib/types.d.ts",
  "description": "A lightweight static site generator based on RSPress, optimized for remote GitHub repositories with TanStack Router and Drizzle ORM",
  "module": "./lib/index.js",
  "main": "./lib/index.js",
  "keywords": [
    "static-site-generator",
    "blog",
    "mdx",
    "markdown",
    "tanstack-router",
    "drizzle-orm",
    "rspress",
    "github"
  ],
  "dependencies": {
    "@hono/node-server": "^1.15.0",
    "@swc/helpers": "~0.5.11",
    "@tanstack/db": "^0.0.14",
    "esbuild": "^0.25.4",
    "hono": "^4.8.3",
    "mdx-bundler": "10.1.1",
    "node-fetch": "^3.3.2"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/types.d.ts"
    },
    "./collection": {
      "import": "./lib/collection.ts"
    },
    "./loaders": {
      "import": "./lib/loaders.ts"
    }
  },
  "bin": {
    "adenta-mdx-bundle": "./bin/cli.js"
  },
  "files": [
    "bin",
    "README.md"
  ],
  "scripts": {
    "test": "vitest",
    "prepublishOnly": "npm run build",
    "examples:build": "vite build examples --outDir ../dist",
    "examples:start": "node examples/mdx.server.ts",
    "examples:run": "npm run examples:build && npm run examples:start"
  },
  "author": "Israel Agyeman-Prempeh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/static-blog-generator.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/static-blog-generator/issues"
  },
  "homepage": "https://github.com/yourusername/static-blog-generator#readme",
  "devDependencies": {
    "@rspack/cli": "^1.3.11",
    "@types/node": "^20.0.0",
    "@vitejs/plugin-react": "^4.6.0",
    "esbuild": "0.*",
    "html-webpack-plugin": "^5.6.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rspack": "^0.1.1",
    "ts-loader": "^9.5.2",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.3.5",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}